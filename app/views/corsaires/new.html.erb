<h1>New corsaire </h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @corsaire  do |f| %>
<%= render 'shared/error_messages' %>

      <%= f.label :first_name %>
      <%= f.text_field :first_name %>

      <%= f.label :age %>
      <%= f.text_field :age %>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
